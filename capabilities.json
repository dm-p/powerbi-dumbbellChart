{
    "dataRoles": [
        {
            "displayName": "Category Data",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Measure Data",
            "name": "measure",
            "kind": "Measure"
        }
    ],
    "objects": {
        "dataPoints": {
            "displayName": "Data Points",
            "properties": {
                "radius": {
                    "displayName": "Radius",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "connectingLines": {
            "displayName": "Connecting Lines",
            "properties": {
                "strokeWidth": {
                    "displayName": "Width",
                    "type": {
                        "numeric": true
                    }
                },
                "color": {
                    "displayName": "Color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "dataLabels": {
            "displayName": "Data Labels",
            "properties": {
                "show": {
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            }
                        }
                    ]
                }
            }
        }
    ]
}
